<h1>Wikinimous</h1>
<% @articles.each do |article| %>
    <article>
    <h2>
      <%= link_to article.title, article_path(article) %>
      <%= link_to article_path(article), method: :delete do %>
        <i class="fa fa-trash"></i>
      <% end %>
    </h2>
    <p><%= article.content %></p>
  </article>
<% end %>
